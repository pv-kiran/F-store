         <div class="dashboard">
            <table width="100%">
                <thead>
                    <tr>
                        <td>PRODUCT</td>
                        <td>STOCK</td>
                        <td>PRICE</td>
                        <td>Image</td>
                        <td>Status</td>
                        <td>Action</td>
                    </tr>
                </thead>
                <tbody>
                    {{#each products}}
                    <tr>
                        <td>{{productName}}</td>
                        <td>{{stock}}</td>
                        <td>{{price}}</td>
                        <td><img src="{{images.[0].secured_url}}" alt="IMAGE"></td>
                        {{#if isBlocked}}
                          <td>Blocked</td>
                        {{else}}
                           <td>Active</td>
                        {{/if}}
                        <td class="product-action">
                            <a href="#" class="btn-edit">
